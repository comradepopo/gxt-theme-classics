<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.7.0//EN"
    "http://gwtproject.org/doctype/2.7.0/gwt-module.dtd">
<module>
  <inherits name="com.google.gwt.xml.XML" />
  <inherits name="com.sencha.gxt.core.Core" />

  <!-- fake the theme styles, dont use a full-on theme -->
  <replace-with class="com.sencha.gxt.core.client.MockThemeAppearance">
    <when-type-is class="com.sencha.gxt.core.client.resources.ThemeStyles.ThemeAppearance" />
  </replace-with>

  <source path="client" />
  <source path="shared" />

  <collapse-all-properties />
</module>
