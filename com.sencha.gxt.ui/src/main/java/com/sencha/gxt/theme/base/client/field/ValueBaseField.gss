
.focus input,
.focus textarea {
  border: 1px solid #7eadd9;
}

.invalid input,
.invalid textarea {
  gwt-sprite: "invalidLine";
  background-color: #fff;
  background-position: bottom;
  border: 1px solid #c30;
  height: 18px;
  line-height: 18px;
}

.wrap {
  position: relative;
  left: 0;
  top: 0;
  zoom: 1;
  white-space: nowrap;
  text-align: left;
}

.field {
  font: 12px tahoma, arial, helvetica, sans-serif;
}

.empty {
  color: gray;
}

.readonly {
}
